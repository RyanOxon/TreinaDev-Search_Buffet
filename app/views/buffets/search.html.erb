<div class="content">
  <h1 class="title is-5">Resultados da Busca por: <%=@search%></h1>
  <p class="subtitle"> <%= @buffets.count %> buffet encontrados </p>
  <% @buffets.each do |buffet| %>
    <p><%= link_to buffet.brand_name, buffet_path(buffet), class: "button is-primary" %></p>
  <% end %>
</div>